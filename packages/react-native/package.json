    "@eslint/js": "^9.16.0",
    "@sf-digital-ui/eslint-config": "*",
    "@sf-digital-ui/tokens": "*",
    "@types/react": "^18.3.12",
    "@types/react-native": "^0.72.0",
    "nodemon": "^3.1.9",
